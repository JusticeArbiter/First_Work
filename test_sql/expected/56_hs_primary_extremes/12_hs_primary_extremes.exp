CREATE OR REPLACE FUNCTION hs_locks_drop (n integer)  RETURNS void  LANGUAGE plpgsql  AS $$  BEGIN  IF n <= 0 THEN  CHECKPOINT;  RETURN;  END IF;  EXECUTE 'DROP TABLE IF EXISTS hs_locks_' || n::text;  PERFORM hs_locks_drop(n - 1);  RETURN;  EXCEPTION WHEN raise_exception THEN NULL; END;  $$;
